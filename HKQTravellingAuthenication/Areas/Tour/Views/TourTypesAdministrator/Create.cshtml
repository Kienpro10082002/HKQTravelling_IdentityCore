@model HKQTravellingAuthenication.Models.Tour.TourTypes

@{
    ViewData["Title"] = "Tạo mới loại tour";
    Layout = "_Layout";
}

@* @section Breadcrumb {
    @{
        ViewData["BreadcrumbTitle"] = "Create for start location";
    }
} *@

<h2>Create</h2>

<h4>TourTypes</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" asp-controller="TourTypesAdministrator" asp-area="Tour" method="post">
            <div class="form-group">
                <label class="control-label" for="TourTypeName">Tên loại tour</label>
                <input class="form-control" type="text" id="TourTypeName" name="TourTypeName" />
                <p class="text-danger">@ViewData["validation_message"]</p>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="@Url.Action("Index", "TourTypesAdministrator", new { area = "Tour" })">Trở lại trang</a>
</div>

