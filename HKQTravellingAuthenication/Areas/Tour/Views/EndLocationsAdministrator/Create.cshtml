@model HKQTravellingAuthenication.Models.Tour.EndLocations

@{
    ViewData["Title"] = "Tạo mới điểm đến";
    Layout = "~/Areas/Views/Shared/_TourManagerLayout.cshtml";
}

@section Breadcrumb {
    @{
        ViewData["BreadcrumbTitle"] = "Create for end location";
    }
}

<h2>Create</h2>

<h4>EndLocations</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" asp-controller="EndLocationsAdministrator" asp-area="Admin" method="post">
            <div class="form-group">
                <label asp-for="EndLocationName" class="control-label"></label>
                <input asp-for="EndLocationName" class="form-control" name="EndLocationName" />
                <p class="text-danger">@ViewData["validation_message"]</p>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="@Url.Action("Index", "EndLocationsAdministrator", new { area = "Tour" })">Back to List</a>
</div>

